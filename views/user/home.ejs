        <div class="container-fluid pt-5">
            <div class="text-center mb-4">
                <!-- <h2 class="section-title px-5"><span class="px-2">New Arrivals</span></h2> -->
            </div>
            <div class="row px-xl-5 pb-3">
                <% for (let i = 0; i < products.length; i++) { %>
                <div class="col-lg-3 col-md-6 col-sm-12 pb-1">
                    <div class="card product-item border-0 mb-4">
                        <div class="card-header product-img position-relative overflow-hidden bg-transparent border p-0">
                            <a href="/productDetails?id=<%= products[i]._id %>">
                                <% if (products[i].productImage && products[i].productImage.length > 0) { %>
                                    <img class="img-fluid w-100" src="/uploads/re-image/<%= products[i].productImage[0] %>" alt="<%= products[i].productName %>">
                                  <% } else { %>
                                    <img class="img-fluid w-100" src="/uploads/product-images/default.jpg" alt="Default Product Image">
                                  <% } %>
                                  
                            </a>
                        </div>
                        <div class="card-body border-left border-right text-center p-0 pt-4 pb-3">
                            <h6 class="text-truncate mb-3"><%= products[i].productName %></h6>
                            <div class="d-flex justify-content-center">
                                <h6>$<%= products[i].salePrice %></h6>
                                <h6 class="text-muted ml-2"><del>$<%= products[i].regularPrice %></del></h6>
                            </div>
                        </div>
                        <div class="card-footer d-flex justify-content-between bg-light border">
                            <a href="/productDetails?id=<%= products[i]._id %>" class="btn btn-sm text-dark p-0"><i class="fas fa-eye text-primary mr-1"></i>View Detail</a>
                            <a href="/cart?id=<%=products[i]._id%>" class="btn btn-sm text-dark p-0"><i class="fas fa-shopping-cart text-primary mr-1"></i>Add To Cart</a>
                        </div>
                    </div>
                </div>
                <% } %>
            </div>
        </div>
